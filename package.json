{
  "name": "express-boilerplate",
  "private": true,
  "scripts": {
    "start": "node ./app/bin/www",
    "server": "nodemon ./server/bin/www",
    "client": "cd client && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "@sanity/cli": "^2.23.0",
    "@sanity/client": "^2.23.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "express": "^4.17.1",
    "http-errors": "~1.7.2",
    "morgan": "~1.9.1",
    "nodemon": "^1.18.11",
    "pg-promise": "^8.5.6",
    "pug": "^2.0.3",
    "sanity": "^2.20.0",
    "sass": "^1.44.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "concurrently": "^4.1.2"
  }
}
